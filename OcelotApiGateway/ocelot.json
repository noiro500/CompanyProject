{
  "GlobalConfiguration": {
    "BaseUrl": "https://localhost:5000"
  },
  "Routes": [
    {
      //"ServiceName": "CompanyProjectContentService",
      "UpstreamPathTemplate": "/gateway/v1/Content/{pageName}",
      "UpstreamHttpMethod": [ "Get" ],
      "DownstreamPathTemplate": "/api/v1/Content/{pageName}",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
        {
          "Host": "localhost",
          "Port": 5010
        }
      ],
      "FileCacheOptions": {
        "TtlSeconds": 10
      }
    },
    {
      "UpstreamPathTemplate": "/gateway/v1/Card/{pageNameForCard}",
      "UpstreamHttpMethod": ["Get"],
      "DownstreamPathTemplate": "/api/v1/Card/{pageNameForCard}",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
        {
          "Host": "localhost",
          "Port": 5011
        }
      ],
      "FileCacheOptions": {
        "TtlSeconds": 10

      }
    }
  ]
}
